{
  "name": "@auroraisnear/cli",
  "version": "0.0.4",
  "description": "Aurora Engine command-line interface (CLI).",
  "type": "module",
  "module": "lib/aurora.js",
  "main": "lib/aurora.js",
  "types": "lib/aurora.d.ts",
  "files": [
    "AUTHORS",
    "CHANGES.md",
    "LICENSE",
    "README.md",
    "VERSION",
    "package.json",
    "lib/aurora.js",
    "lib/aurora.d.ts"
  ],
  "bin": {
    "aurora": "lib/aurora.js"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest",
    "format": "prettier -w src",
    "lint": "eslint **/*.ts"
  },
  "keywords": [
    "Aurora",
    "Ethereum",
    "NEAR"
  ],
  "author": "aurora.dev",
  "authors": [
    "Arto Bendiken <arto@near.org> (https://ar.to)"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/aurora-is-near/aurora-cli",
  "bugs": "https://github.com/aurora-is-near/aurora-cli/issues",
  "repository": "https://github.com/aurora-is-near/aurora-cli.git",
  "dependencies": {
    "console-table-printer": "^2.9.0",
    "@aurora-is-near/engine": "aurora-is-near/aurora.js#ee7dd4c",
    "commander": "^7.2.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "prettier": "2.2.1",
    "ts-jest": "^28.0.7",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[tj]sx?$": "ts-jest"
    }
  }
